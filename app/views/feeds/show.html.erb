

<div class="constrainer">
  <div class="col-xs-12 col-sm-9 user-page-body">

    <div class = "stream" >
      <%= render partial: "streams/stream_main", locals: {streams: @streams, nest_count: 0} %>
    </div>
    <div id = "infinite-scrolling" >
      <%= will_paginate @streams %>
    </div>

  </div>

  <div class="col-sm-3 stockpage-sidebar">
    <%= render partial: 'shared/top_stocks', locals: {popular_stocks: @popular_stocks} %>
  </div>

</div>

