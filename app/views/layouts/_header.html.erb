        <i class="fa fa-chevron-down"></i>
      </a>
    </div>
    <div class="nav-items">
      <div class="nav-items-icons pull-right">
        <%= link_to "/settings/", class: "nav-settings pull-right" do %>
          <i class="fa fa-cog"></i>
        <% end %>
