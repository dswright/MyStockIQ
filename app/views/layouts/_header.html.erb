        <%= link_to "/users/#{current_user.username}/" do %>
          <%= image_tag current_user.image %>
          <p><%= current_user.username %></p>
        <% end %>
      </div>
      <div class="nav-items-feed pull-right">
        <%= link_to "Feed", feed_path, id: "feed" %>
      </div>    

