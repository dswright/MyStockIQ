<div class="prediction-name">

	<div class="prediction-name-center clearfix">
		<div class="prediction-name-items pull-left">
	  	<%= image_tag prediction.user.image %>
	  	<span class="prediction-name-title"><span class="prediction-name-title-name"><%= prediction.user.username %></span> Prediction</span>
	  </div>
		<div class="prediction-name-vote-count pull-left">
			<a class="prediction-name-vote-count-up" href="#"><i class="fa fa-angle-up"></i></a>
			<p>10</p>
			<a class="prediction-name-vote-count-down" href="#"><i class="fa fa-angle-down"></i></a>
		</div>
	</div>

</div>